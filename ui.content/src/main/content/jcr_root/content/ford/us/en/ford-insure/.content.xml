                        titleFontSize="50px"
                        titleFontWeight="500"/>
                    <accordion_list
                        jcr:created="{Date}2025-05-07T14:43:53.753+05:30"
                        jcr:createdBy="admin"
                        jcr:lastModified="{Date}2025-05-07T15:00:51.131+05:30"
